{
	"name": "spencer",
	"version": "1.0.0",
	"description": "Spencer is a discord bot developed by Salvage_Dev.",
	"main": "dist/index.js",
	"scripts": {
		"dev": "yarn analyze && ts-node-dev --respawn --transpile-only --poll ./src/index.ts",
		"dev:language": "ts-node-dev --respawn --transpile-only --poll ./src/language/test.ts",
		"lint": "prettier --write .",
		"build": "tsc --diagnostics",
		"update": "git pull && yarn && yarn start:core && pm2 reload Spencer --update-env && pm2 logs Spencer",
		"analyze": "ts-node ./src/analyzers/Analyze.ts",
		"new": "ts-node ./src/scripts/New.ts",
		"nb": "git checkout master && git pull && git checkout -b",
		"start": "node ."
	},
	"license": "Creative Commons",
	"dependencies": {
		"@discordjs/builders": "^1.2.0",
		"@discordjs/rest": "^1.2.0",
		"consola": "^2.15.3",
		"discord-api-types": "^0.37.8",
		"discord.js": "^14.3.0",
		"dotenv": "^16.0.2",
		"mongoose": "^6.6.0",
		"ms": "^2.1.3",
		"node-fetch": "^3.2.10",
		"tslib": "^2.4.0"
	},
	"devDependencies": {
		"@types/glob": "^8.0.0",
		"@types/mongoose": "^5.10.3",
		"@types/ms": "^0.7.31",
		"@types/node": "^18.7.16",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"typescript": "^4.8.3"
	}
}
