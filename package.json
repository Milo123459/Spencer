{
	"name": "spencer",
	"version": "1.0.0",
	"description": "Spencer is a discord bot developed by Salvage_Dev.",
	"main": "dist/src/index.js",
	"scripts": {
		"dev": "yarn analyze && ts-node-dev --respawn --transpile-only --poll ./src/index.ts",
		"dev:language": "ts-node-dev --respawn --transpile-only --poll ./src/language/test.ts",
		"lint": "prettier --write .",
		"build": "tsc --diagnostics",
		"start:core": "del-cli ./dist && yarn build",
		"update": "n latest && git pull && yarn && yarn start:core && pm2 reload Spencer --update-env && pm2 logs Spencer",
		"analyze": "ts-node ./src/analyzers/Analyze.ts",
		"doctor": "doctor",
		"sdk": "yarn pnpify --sdk",
		"gbump": "yarn bump && yarn sdk && yarn git 'chore(Deps): bump deps'",
		"new": "ts-node ./src/scripts/New.ts",
		"nb": "git checkout master && git pull && git checkout -b",
		"start": "yarn start:core && pm2 start dist/src/index.js --name Spencer"
	},
	"license": "Creative Commons",
	"dependencies": {
		"@top-gg/sdk": "^3.0.9",
		"alpha-sort": "^5.0.0",
		"bufferutil": "^4.0.3",
		"cleverbot-free": "^1.0.5",
		"consola": "^2.15.3",
		"dayjs": "^1.10.4",
		"discord.js": "^12.5.1",
		"erlpack": "^0.1.3",
		"express": "^4.17.1",
		"glob": "^7.1.6",
		"imageapi.js": "^1.6.1",
		"leven": "^3.1.0",
		"mathjs": "^9.3.2",
		"mongoose": "^5.12.5",
		"ms": "^2.1.3",
		"node-fetch": "^2.6.1",
		"os-name": "^4.0.0",
		"prompts": "^2.4.1",
		"tslib": "^2.1.0",
		"tyvn": "^1.8.0",
		"utf-8-validate": "^5.0.4",
		"vacefron": "^3.4.0",
		"yn": "^4.0.0",
		"zlib-sync": "^0.1.7"
	},
	"devDependencies": {
		"@types/express": "^4.17.11",
		"@types/glob": "^7.1.3",
		"@types/mathjs": "^9.4.2",
		"@types/mongoose": "^5.10.3",
		"@types/ms": "^0.7.31",
		"@types/node": "^15.6.2",
		"@types/node-fetch": "^2.5.8",
		"@types/prettier": "^2.2.2",
		"@types/prompts": "^2.0.9",
		"del-cli": "^3.0.1",
		"prettier": "^2.2.1",
		"ts-node": "^10.0.0",
		"ts-node-dev": "^1.1.6",
		"typescript": "^4.3.2"
	}
}
