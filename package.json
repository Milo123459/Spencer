{
	"name": "spencer",
	"version": "1.0.0",
	"description": "Spencer is a discord bot developed by Salvage_Dev.",
	"main": "dist/src/index.js",
	"scripts": {
		"dev": "yarn analyze && ts-node-dev --respawn --transpile-only --poll ./src/index.ts",
		"dev:language": "ts-node-dev --respawn --transpile-only --poll ./src/language/test.ts",
		"lint": "prettier --write .",
		"build": "tsc --diagnostics",
		"start:core": "del-cli ./dist && yarn && yarn build",
		"update": "n latest && git pull && yarn && yarn start:core && pm2 reload Spencer --update-env && pm2 logs Spencer",
		"analyze": "ts-node ./src/analyzers/Analyze.ts",
		"new": "ts-node ./src/scripts/New.ts",
		"nb": "git checkout master && git pull && git checkout -b",
		"start": "yarn start:core && pm2 start dist/src/index.js --name Spencer"
	},
	"license": "Creative Commons",
	"dependencies": {
		"@discordjs/builders": "^0.8.2",
		"@discordjs/rest": "^0.1.0-canary.0",
		"@top-gg/sdk": "^3.1.2",
		"bufferutil": "^4.0.5",
		"cleverbot-free": "^1.1.7",
		"consola": "^2.15.3",
		"dayjs": "^1.10.6",
		"discord-api-types": "^0.24.0",
		"discord.js": "^13.3.1",
		"express": "^4.17.1",
		"glob": "^7.1.6",
		"leven": "^4.0.0",
		"mathjs": "^10.0.0",
		"mongoose": "^6.0.13",
		"ms": "^2.1.3",
		"node-fetch": "^3.1.0",
		"prompts": "^2.4.1",
		"tslib": "^2.3.0",
		"tyvn": "^1.8.0",
		"vacefron": "^3.4.0",
		"yn": "^5.0.0"
	},
	"devDependencies": {
		"@types/express": "^4.17.13",
		"@types/glob": "^7.2.0",
		"@types/mathjs": "^9.4.2",
		"@types/mongoose": "^5.10.3",
		"@types/ms": "^0.7.31",
		"@types/node": "^17.0.17",
		"@types/prettier": "^2.4.2",
		"@types/prompts": "^2.0.14",
		"del-cli": "^4.0.1",
		"prettier": "^2.3.2",
		"ts-node": "^10.4.0",
		"ts-node-dev": "^1.1.8",
		"typescript": "^4.4.4",
		"yarn-deduplicate": "^3.1.0"
	}
}
